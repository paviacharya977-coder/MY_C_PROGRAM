#include<stdio.h>
int main (){
    int largest (int a, int b, int c){
        if(a>=b && a>=c){
            printf("a is largest");
        }
        if (b>=a && b>=c){
            printf("b is largest");
        }
        else
        (c>=b && c>=a);{
        printf("c is largest");
        }
    int main (){
        largest(2,3,5);}
    }
}
