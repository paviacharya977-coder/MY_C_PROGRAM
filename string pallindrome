#include<stdio.h>
#include<string.h>
int main(){
    int num,original,rem,rev=0;
    printf("enter a number:");
    scanf("%d",&num);
    original=num;
    while(num!=0){
        rem=num%10;
        rev=rev*10+rem;
        num/=10;
    }
    if(original==rev)
    printf("it is a pallindrome");
    else
    printf("not a pallindrome");
}


OUTPUT
enter a number:2
it is a pallindrome
